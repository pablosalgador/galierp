{% extends 'admin.html.twig' %}
{% block content %}
<h1 class="h1 mb-2 font-weight-bold text-gray-800"><i class="fas fa-fw fa-receipt"></i> &nbsp;{% trans %}Nuevo gasto{% endtrans %}</h1>
<hr/>
{% for msg in msg_success %}
<div class="alert alert-success border-bottom-success font-weight-bold"><i class="fa fa-check fa-fw"></i> &nbsp;{{msg}}</div>
{% endfor %}
<div class="card shadow p-4">

  {{ form_start(form, {attr: {class: 'user'} }) }}
  {{ form_errors(form) }}

  {{ form_widget(form) }}
  {{ form_end(form) }}

</div><!-- END: .card .shadow-->

{% endblock %}
